<view class="container">
  <view class="title0">足迹地图</view>
  <ec-canvas class="canvas" id="mychart-dom-area" canvas-id="mychart-area" ec="{{ ec }}"></ec-canvas>
  <view> 
    <open-data class="icon" type="userAvatarUrl"></open-data>
    <open-data  class="name" type="userNickName"></open-data>
    <view class="rank">足迹超越<text>{{percentage}}%</text>的用户</view>
    <view class="senten">世界这么大，多出门走走</view>    
    <button open-type='share' class='button'><image class='pic' src="/images/share.png"></image></button>
  </view>
</view>
