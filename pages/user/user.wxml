<view class='toptop'>

  <view class='touxiang'>
    <image src='{{avatarUrl}}'></image>
  </view>

  <text class='mingzi'>{{nickName}}</text>

  <view class="text1">
    <text>国家：{{country}}</text>
    <text>省份：{{province}}</text>
    <text>城市：{{city}}</text>
  </view>

</view>

<view class='chengjiu'>
  <view wx:for='{{achieve}}' class='chengjiu1'>
    <image src='{{item.src}}' bindtap='link' class='left'></image>
    <view class='left'>{{item.content}}</view>
  </view>
</view>
