<view class="bg"  >
<map class="mapcontainer" longitude="{{longitude}}" latitude="{{latitude}}" markers='{{markers}}' bindcallouttap="ct">
  
</map>
  <cover-view class="bt1">
    <cover-image bindtap="navi" data-path="map" src="/images/icon/switch.png" class="img-plus-style"/>
  </cover-view>
  <cover-view bindtap="navi" data-path="photo" class="bt2" id='home-action-wrapper' style="left:{{windowWidth}}">
    <cover-image src="/images/icon/position.png" class="img-plus-style"/>
  </cover-view>
  <cover-view class="bt3">
    <cover-image src="/images/icon/plus.png" class="img-plus-style" animation="{{animPlus}}"  bindtap="plus"  />
  </cover-view>
    <cover-image bindtap="navi" data-path="model_test" src="/images/icon/story.png" class="img-style" animation="{{animCollect}}"/>
    <cover-image bindtap="navi" data-path="user" src="/images/icon/me.png" class="img-style" animation="{{animInput}}"/>
    <!-- <cover-image src="/images/icon/me.png" class="img-style" animation="{{animTranspond}}" bindtap="transpond"/> -->
    <!-- <cover-image src="/images/icon/me.png" class="img-style" animation="{{animInput}}" bindtap="input"/> -->
  
</view>